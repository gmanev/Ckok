<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="1.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<entity class="net.nbt.ckok.model.Product">
		<table name="CKOK_PRODUCTS" />
		<attributes>
			<id name="id">
				<column name="PRODUCT_ID" />
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="notes">
			</basic>
			<basic name="supplier">
			</basic>
			<basic name="serial">
			</basic>
			<basic name="quantity">
			</basic>
			<many-to-one name="productType" target-entity="net.nbt.ckok.model.ProductType">
			</many-to-one>
		</attributes>
	</entity>
	<entity class="net.nbt.ckok.model.ProductType">
		<table name="CKOK_NOMEN" />
		<attributes>
			<id name="id">
				<column name="NOMEN_ID" />
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="partnum">
			</basic>
			<basic name="measure">
			</basic>
			<basic name="name">
			</basic>
		</attributes>
	</entity>
</entity-mappings>
